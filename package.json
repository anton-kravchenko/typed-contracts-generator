  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "caporal": "^0.10.0",
    "flow-bin": "^0.84.0",
